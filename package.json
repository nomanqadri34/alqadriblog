{
  "name": "qadri-blog",
  "version": "1.0.0",
  "description": "Full stack blog application",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-api": "cd api && npm install",
    "install": "npm run install-client && npm run install-api",
    "build-client": "cd client && npm run build",
    "build-api": "cd api && npm run build",
    "build": "npm run build-client && npm run build-api",
    "start": "cd api && npm start",
    "dev": "concurrently \"cd api && npm run dev\" \"cd client && npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
